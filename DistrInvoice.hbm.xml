<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="ph.dal.entities.documents.DistrInvoice" table="OrderDistrInvoices">
    <id column="Id" name="id">
      <generator class="identity"/>
    </id>

    <many-to-one class="ph.dal.entities.documents.Order"   column="OrderId"   name="order"   not-null="true" fetch="join"/>
    <many-to-one class="ph.dal.entities.references.Client"  column="ClientId"  name="client"   not-null="true" fetch="join"/>
    <many-to-one class="ph.dal.entities.staff.Employee"  column="EmployeeId"  name="employee"   fetch="join"/>
    <many-to-one class="ph.dal.entities.documents.TransporterCars"   column="CarId"   name="car"   not-null="false" fetch="join"/>
    <many-to-one class="ph.dal.entities.documents.TransporterDrivers"  column="DriverId"  name="driver"   not-null="false" fetch="join"/>
    <many-to-one class="ph.dal.entities.documents.Transporters"  column="TrasporterId"  name="transporter"  not-null="false"  fetch="join"/>
    <many-to-one class="ph.dal.entities.references.ClientDistributAdress"  column="ClientAdressId"  name="clientAdress"   not-null="false" fetch="join"/>
    <property column="CopyCount"                 name="copyCount"/>
    <property column="copiesPerStandartPack"     name="copiesPerStandartPack"/>
    <property column="standartPackCount"         name="standartPackCount"/>
    <property column="copiesPerNonStandartPack"  name="copiesPerNonStandartPack"/>
    <property column="docDate"                   name="docDate"/>
    <property column="readyDate"                 name="readyDate"/>
    <property column="shipmentDate"              name="shipmentDate"/>
    <property column="ClientAddInfo"             name="clientAddInfo"/>
    <property column="Comment"                   name="comment"/>
  </class>  
</hibernate-mapping>
